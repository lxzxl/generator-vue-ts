{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}
